<div class="work-order"
     style="height: <%= work_order.duration - 9%>px"
     data-start-minutes="<%= start_min %>"
     data-end-minutes="<%= start_min + work_order.duration %>"
     >
  <b><%= work_order.location.name %></b>
  <br>
  <%= work_order.location.city %> at <%= work_order.time.split[1] %>
  <br>
  <%= work_order.duration %> minutes
  $<%= work_order.price %>
</div>
